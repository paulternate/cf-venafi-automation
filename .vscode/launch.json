{
    "configurations": [
        {
            "type": "aws-sam",
            "request": "direct-invoke",
            "name": "tlspc-application:Function",
            "invokeTarget": {
                "target": "template",
                "templatePath": "${workspaceFolder}/tlspc/functions/tlspc-application/template.yaml",
                "logicalId": "Function"
            },
            "lambda": {
                "payload": {
                    "path": "${workspaceFolder}/tlspc/functions/tlspc-application/events/event.json"
                },
                "environmentVariables": {
                    "PYDEVD_WARN_SLOW_RESOLVE_TIMEOUT": "600"
                }
            }
        },
        {
            "type": "aws-sam",
            "request": "direct-invoke",
            "name": "tlspc-certificate:Function",
            "invokeTarget": {
                "target": "template",
                "templatePath": "${workspaceFolder}/tlspc/functions/tlspc-certificate/template.yaml",
                "logicalId": "Function"
            },
            "lambda": {
                "payload": {
                    "path": "${workspaceFolder}/tlspc/functions/tlspc-certificate/events/event.json"
                },
                "environmentVariables": {
                    "PYDEVD_WARN_SLOW_RESOLVE_TIMEOUT": "600"
                }
            }
        },
        {
            "type": "aws-sam",
            "request": "direct-invoke",
            "name": "do-nothing:Function",
            "invokeTarget": {
                "target": "template",
                "templatePath": "${workspaceFolder}/tlspc/functions/do-nothing/template.yaml",
                "logicalId": "Function"
            },
            "lambda": {
                "payload": {
                    "path": "${workspaceFolder}/tlspc/functions/do-nothing/events/event.json"
                },
                "environmentVariables": {
                    "PYDEVD_WARN_SLOW_RESOLVE_TIMEOUT": "600"
                }
            }
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": "false",
            "env": {
                "TARGET_STACK_ID": "arn:aws:cloudformation:us-east-1:427380916706:stack/amcginlay-cert-7718/b9f260e0-f57d-11ed-9fa9-12bb237a89b1"
            }
        }
    ]
}