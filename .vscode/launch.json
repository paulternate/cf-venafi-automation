{
    "configurations": [
        {
            "type": "aws-sam",
            "request": "direct-invoke",
            "name": "do-nothing:Function",
            "invokeTarget": {
                "target": "template",
                "templatePath": "${workspaceFolder}/tlspc/functions/do-nothing/template.yaml",
                "logicalId": "Function"
            },
            "lambda": {
                "payload": {
                    "path": "${workspaceFolder}/tlspc/functions/do-nothing/events/event.json"
                },
                "environmentVariables": {}
            }
        }
    ]
}